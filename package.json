    "ember-resolver": "^4.0.0",
    "ember-router-service-polyfill": "^1.0.1",
    "ember-source": "~2.12.0",
    "ember-try": "0.2.22",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "6.* || >= 7.*"
