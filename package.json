{
  "name": "ember-cli-markdown-resolver",
  "version": "0.1.1",
  "description": "Ember CLI addon for resolving markdown files in custom folders and retrieving content via a service.",
  "keywords": [
    "ember-addon",
    "markdown",
    "md",
    "resolver",
    "frontmatter",
    "content"
  ],
  "license": "MIT",
  "author": {
    "name": "Will Viles",
    "email": "will@vil.es",
    "url": "willviles.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willviles/ember-cli-markdown-resolver.git"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout master"
  },
  "dependencies": {
